{
  "name": "react-primitives-calendar",
  "version": "0.0.1",
  "description": "React DatePicker using react-primitives to allow rendering on any platform.",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "jsnext:main": "./es/index.js",
  "types": "./index.d.ts",
  "author": "Doug Miller <doug@dougmiller.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "storybook": "run-p storybook:web storybook:rn",
    "storybook:web": "start-storybook -p 9001 -c .storybook",
    "storybook:rn": "haul start --platform android --config webpack.haul.storybook.js",
    "start": "haul start",
    "build": "yarn build:es && yarn build:cjs",
    "build:cjs": "tsc --outDir lib --m CommonJS",
    "build:es": "tsc --outDir es -m ESNext -d",
    "lint": "tslint src/** stories/** storybook-rn/** storybook-web/**"
  },
  "dependencies": {
    "@types/react": "^16.0.10",
    "@types/react-native": "^0.49.1",
    "aphrodite": "^1.2.5",
    "npm-run-all": "^4.1.1",
    "react-bits": "^1.0.0",
    "react-native-svg": "^5.4.2",
    "react-primitives": "^0.4.3",
    "react-primitives-svg": "^0.0.2",
    "react-with-styles": "^2.2.0",
    "react-with-styles-interface-aphrodite": "^3.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-links": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "@storybook/react-native": "^3.2.12",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "create-react-class": "^15.6.2",
    "haul": "^1.0.0-beta.6",
    "jest": "21.2.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-native": "^0.49.3",
    "react-native-web": "^0.1.7",
    "react-test-renderer": "16.0.0-beta.5",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.5.3"
  },
  "peerDependencies": {}
}
